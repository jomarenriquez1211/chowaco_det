{
  "type": "object",
  "properties": {
    "summary": {
      "type": "object",
      "properties": {
        "totalGoals": { "type": "number" },
        "totalBMPs": { "type": "number" },
        "completionRate": { "type": "number", "minimum": 0, "maximum": 100 }
      },
      "required": ["totalGoals", "totalBMPs", "completionRate"]
    },
    "goals": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": { "type": "string" },
          "description": { "type": ["string", "null"] },
          "status": { "type": ["string", "null"], "enum": ["Not Started", "In Progress", "Completed", null] },
          "target": { "type": ["string", "null"] },
          "progress": { "type": ["number", "null"], "minimum": 0, "maximum": 100 }
        },
        "required": ["title", "status", "progress"]
      }
    },
    "bmps": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": { "type": "string" },
          "description": { "type": ["string", "null"] },
          "category": { "type": ["string", "null"] },
          "quantity": { "type": ["number", "null"] },
          "cost": { "type": ["number", "null"] }
        },
        "required": ["title", "quantity", "cost"]
      }
    },
    "implementation": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "activity": { "type": "string" },
          "description": { "type": ["string", "null"] },
          "startDate": { "type": ["string", "null"], "format": "date" },
          "endDate": { "type": ["string", "null"], "format": "date" },
          "status": { "type": ["string", "null"], "enum": ["Not Started", "In Progress", "Completed", null] }
        },
        "required": ["activity", "status"]
      }
    },
    "monitoring": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "metricName": { "type": "string" },
          "value": { "type": ["string", "null"] },
          "units": { "type": ["string", "null"] },
          "description": { "type": ["string", "null"] },
          "baseline": { "type": ["string", "null"] },
          "target": { "type": ["string", "null"] }
        },
        "required": ["metricName", "value", "units"]
      }
    },
    "outreach": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "activity": { "type": "string" },
          "description": { "type": ["string", "null"] },
          "count": { "type": ["number", "null"] }
        },
        "required": ["activity", "count"]
      }
    },
    "geographicAreas": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "description": { "type": ["string", "null"] },
          "acres": { "type": ["number", "null"] },
          "croplandPct": { "type": ["number", "null"], "minimum": 0, "maximum": 100 },
          "wetlandPct": { "type": ["number", "null"], "minimum": 0, "maximum": 100 }
        },
        "required": ["name", "acres"]
      }
    }
  },
  "required": ["summary", "goals", "bmps", "implementation", "monitoring", "outreach", "geographicAreas"]
}
